       "<div style=\"overflow:auto\" id=\"graph-output-e2yKgpnq\"></div>\n",
       "<script type=\"text/javascript\">\n",
       "require.config({baseUrl: \"env/lib/python3.8/site-packages/pyzx/js\",paths: {d3: \"d3.v5.min\"} });\n",
